.container {
  background: #efeff4;
  min-height: 100%;
  align-items: center;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page-body{
  margin-top: 5rpx;
}
.page-head{
  padding: 10rpx 0;
  text-align: center;
}
.page-head-title{
  display: inline-block;
  padding: 0 40rpx 20rpx 40rpx;
  font-size: 32rpx;
  color: #C8C8C8;
}
.page-head-line{
  margin: 0 auto;
  width: 250rpx;
  height: 2rpx;
  background-color: #C8C8C8;
}
